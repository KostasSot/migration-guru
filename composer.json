{
    "name": "nikelioum/migration-guru",
    "description": "A small Laravel 12 package to manage migrations from a web UI",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Nikelioum",
            "email": "dev@nikelioum.local"
        }
    ],
    "require": {
        "php": "^8.1 || ^8.2 || ^8.3",
        "illuminate/support": "^12.0"
    },
    "autoload": {
        "psr-4": {
            "Nikelioum\\MigrationGuru\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nikelioum\\MigrationGuru\\MigrationGuruServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/nikelioum/migration-guru"
        }
    ]
}
