{
    "name": "nikelioum/migration-guru",
    "description": "A small Laravel 12 package to manage migrations from a web UI",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Dimitris Liaropoulos",
            "email": "dimliarop@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1 || ^8.2 || ^8.3",
        "illuminate/support": "^10.0 || ^11.0 || ^12.0"
    },
    "autoload": {
        "psr-4": {
            "Nikelioum\\MigrationGuru\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nikelioum\\MigrationGuru\\MigrationGuruServiceProvider"
            ]
        }
    },
    "keywords": [
        "laravel",
        "migrations",
        "database",
        "ui",
        "migration-guru"
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/nikelioum/migration-guru"
        }
    ]
}
